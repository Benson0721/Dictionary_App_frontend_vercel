{
  "name": "dictionary-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.js",
    "build": "vite build --config vite.config.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "bulid:css": "postcss src/css/index.css --dir ./src/css && postcss src/css/GridSesctions.css --dir ./src/css && postcss src/css/Section.css --dir ./src/css ",
    "tailwind:css": "npx tailwindcss -i ./client/src/assets/css/tailwind.css -o ./client/src/assets/css/generated-tailwind.css --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "npm run server"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "@tailwindcss/vite": "^4.1.8",
    "axios": "^1.7.9",
    "joi": "^17.13.3",
    "localforage": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.2.0",
    "sass": "^1.89.0",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "tailwindcss": "^4.1.8",
    "vite": "^6.3.5"
  }
